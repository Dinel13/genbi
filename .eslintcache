ion/json\",\n      },\n      body: JSON.stringify({\n        query: ` \n          query { \n            pendaftars(adminId: \"${adminId}\" kampus : \"unm\" jenis : \"reguler\") {\n              nama\n              nim\n              fakultas\n              prodi\n              ipk\n              mampu\n            }\n          }`,\n      }),\n    })\n      .then((response) => response.json())\n      .then((data) => {\n        console.log(data);\n        if (data.errors) {\n          throw data.errors[0].message;\n        }\n        setPendaftar(data);\n        setIsLoading(false);\n      })\n      .catch((error) => {\n        setIsLoading(false);\n        setIsError(error);\n        console.log(error);\n      });\n  }, [setActive, adminId, admin]);\n\n  return (\n    <>\n      <div className=\"d-flex justify-content-between flex-wrap card-header m  flex-md-nowrap align-items-center py-3  ps-3 pe-4 border-bottom shadow-sm\">\n        <h1 className=\"h2\">Pendaftar Universitas Negeri Makassar</h1>\n        <div className=\"btn-toolbar mb-2 mb-md-0\" id=\"ggggg\">\n          <div className=\"btn-group me-2\">\n            <button type=\"button\" className=\"btn btn-sm btn-outline-secondary\">\n              Jumlah pendaftar <span className=\"badge bg-primary\">100</span>\n            </button>\n          </div>\n        </div>\n      </div>\n      <Switch>\n        <Route exact path={path}>\n         {pendaftar ? <Tabel Unhas={pendaftar} url={url} /> : <div>tidak ada data</div>}\n        </Route>\n        <Route path={`${path}/:topicId`}>\n          <Pendaftar />\n        </Route>\n      </Switch>\n    </>\n  );\n};\nexport default PendaftarUnm;\n","/home/din/node/genbi/frontend/src/components/admin/LolosWawancara/Card.js",[],"/home/din/node/genbi/frontend/src/components/admin/LolosBerkas/Tabel.js",[],"/home/din/node/genbi/frontend/src/Data/PendaftarUnhas.js",[],"/home/din/node/genbi/frontend/src/Data/BerkasUnhas.js",[],"/home/din/node/genbi/frontend/src/Data/Wawancara